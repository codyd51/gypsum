{% extends "base.html.jinja2" %}

{% block title %}
    Test Title
{% endblock %}

{% block content %}
    <h1>
        {{ generated_at }}
    </h1>
    <h1>
        Receiver timestamp: {{ state.receiver_timestamp }}
    </h1>
    <h1>
        Satellite IDs eligible for acquisition:
    </h1>
    <ul>
        {% for sat_id in state.satellite_ids_eligible_for_acquisition %}
            <li>{{ sat_id }}</li>
        {% endfor %}
    </ul>
    <ul>
        {% for dashboard_image_base64 in state.dashboard_figures %}
            <li><img src="data:image/png;base64, {{ dashboard_image_base64 }}" alt="matplotlib figure visualizing statistics, metrics, and graphs concerning the process of decoding a particular GPS satellite signal from the incoming antenna data." /></li>
        {% endfor %}
    </ul>
{% endblock %}
